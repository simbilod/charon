                          <Parameter name="fact: level-of-fill" type="int" value="1" />
                          <Parameter name="schwarz: reordering type" type="string" value="rcm" />
                          <Parameter name="reuse numeric factorization" type="int" value="1" />
                        </ParameterList>
                      </ParameterList>
                    </ParameterList>
                  </ParameterList>
                </ParameterList>
              </ParameterList>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>
      <ParameterList name="Line Search" >
