<ParameterList name="Heilios">

  <ParameterList name="Mesh">
    <Parameter name="Source" type="string" value="Exodus File" />
    <ParameterList name="Exodus File">
      <Parameter name="File Name" type="string" value="si-diode.nlp.exo" />
      <Parameter name="Restart Index" type="int" value="-1" />
    </ParameterList>
  </ParameterList>

  <ParameterList name="Block ID to Physics ID Mapping">
    <Parameter name="silicon" type="string" value="Semiconductor"/>
  </ParameterList>

  <ParameterList name="Assembly">
    <Parameter name="Workset Size" type="int" value="50"/>
  </ParameterList>

  <ParameterList name="Physics Blocks">

    <ParameterList name="Semiconductor">
      <ParameterList>
        <Parameter name="Type" type="string" value="SGCVFEM Drift Diffusion"/> 
        <Parameter name="Basis Type" type="string" value="HGrad"/>  
        <Parameter name="Basis Order" type="int" value="1"/>
        <Parameter name="Integration Order" type="int" value="2"/> 
        <Parameter name="Model ID" type="string" value="Silicon Parameter"/>  
        <Parameter name="Prefix" type="string" value=""/>
        <ParameterList name="Options">
          <Parameter name="Solve Electron" type="string" value="True" />
          <Parameter name="Solve Hole" type="string" value="True" /> 
          <Parameter name="SRH" type="string" value="On" />
          <Parameter name="Radiative" type="string" value="Off" />
          <Parameter name="Auger" type="string" value="Off" />
          <Parameter name="Band Gap Narrowing" type="string" value="Off" />
          <Parameter name="Fermi Dirac" type="string" value="True" /> 
          <Parameter name="Optical Generation" type="string" value="On" />        
        </ParameterList>
      </ParameterList>      
    </ParameterList>

  </ParameterList>

  <ParameterList name="Closure Models">
    <Parameter name="Lattice Temperature" type="double" value="300"/>
    <Parameter name="Concentration Scaling" type="double" value="1.0e19"/>

    <ParameterList name="Silicon Parameter">
      <Parameter name="Material Name" type="string" value="Silicon"/>

      <ParameterList name="Relative Permittivity">
        <Parameter name="Value" type="double" value="11.9"/>
      </ParameterList>

      <ParameterList name="Doping">
        <Parameter name="Value" type="string" value="Function"/>
       	<!-- p doping -->
        <ParameterList name="Function 1">
          <Parameter name="Function Type" type="string" value="Uniform"/>
          <Parameter name="Doping Type" type="string" value="Acceptor"/>
          <Parameter name="Doping Value" type="double" value="1e18"/>
          <Parameter name="Xmin" type="double" value="0.0"/>
          <Parameter name="Xmax" type="double" value="1.1"/>
	  <Parameter name="Ymin" type="double" value="0.5"/>
	  <Parameter name="Ymax" type="double" value="1.1"/>
        </ParameterList>
	<!-- n doping -->
        <ParameterList name="Function 2">
          <Parameter name="Function Type" type="string" value="Uniform"/>
          <Parameter name="Doping Type" type="string" value="Donor"/>
          <Parameter name="Doping Value" type="double" value="2.5e16"/>
          <Parameter name="Xmin" type="double" value="0.0"/>
          <Parameter name="Xmax" type="double" value="1.1"/>
	  <Parameter name="Ymin" type="double" value="0.0"/>
          <Parameter name="Ymax" type="double" value="0.5"/>
        </ParameterList>
      </ParameterList>

      <ParameterList name="Electron Mobility"> 
        <Parameter name="Value" type="string" value="Arora" />
      </ParameterList> 

      <ParameterList name="Hole Mobility"> 
        <Parameter name="Value" type="string" value="Arora" />
      </ParameterList> 

      <ParameterList name="Electron Lifetime">
         <Parameter name="Value" type="string" value="Function" />
        <ParameterList name="Function 1">
          <Parameter name="Function Type" type="string" value="ConcDep" />
        </ParameterList>
      </ParameterList>

      <ParameterList name="Hole Lifetime">
        <Parameter name="Value" type="string" value="Function" />
        <ParameterList name="Function 1">
          <Parameter name="Function Type" type="string" value="ConcDep" />
        </ParameterList>
      </ParameterList>

      <ParameterList name="Intrinsic Concentration">
        <Parameter name="Value" type="string" value="Old Slotboom" />
      </ParameterList>


      <ParameterList name="Optical Generation">
        <Parameter name="Value" type="string" value="Function"/>
        <ParameterList name="Function 1">
          <Parameter name="Function Type" type="string" value="File"/>
          <Parameter name="File Name" type="string" value="opto_generation.dat"/>
          <Parameter name="X Max" type="double" value="100.0" />
          <Parameter name="X Min" type="double" value="0.0" />
        </ParameterList>
      </ParameterList>


    </ParameterList>

  </ParameterList>

  <ParameterList name="User Data">
    <ParameterList name="function data one">
    </ParameterList>
  </ParameterList>

  <ParameterList name="Boundary Conditions">
    <ParameterList>
      <Parameter name="Type" type="string" value="Dirichlet"/> 
      <Parameter name="Sideset ID" type="string" value="anode"/> 
      <Parameter name="Element Block ID" type="string" value="silicon"/> 
      <Parameter name="Equation Set Name" type="string" value="ALL_DOFS"/> 
      <Parameter name="Strategy" type="string" value="Ohmic Contact"/> 
      <ParameterList name="Data">
        <Parameter name="Voltage" type="double" value="0.0"/>
      </ParameterList>
    </ParameterList>
    
    <ParameterList>
      <Parameter name="Type" type="string" value="Dirichlet"/> 
      <Parameter name="Sideset ID" type="string" value="cathode"/> 
      <Parameter name="Element Block ID" type="string" value="silicon"/> 
      <Parameter name="Equation Set Name" type="string" value="ALL_DOFS"/> 
      <Parameter name="Strategy" type="string" value="Ohmic Contact"/>
      <ParameterList name="Data">
        <Parameter name="Voltage" type="double" value="0.0"/>
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <ParameterList name="Initial Conditions">
    <!-- <Parameter name="Scale Input Exodus Variables" type="bool" value="true"/> -->
    <ParameterList name="silicon">
      <ParameterList name="ELECTRIC_POTENTIAL">
        <Parameter name="Value" type="string" value="Exodus File"/>
      </ParameterList>
      <ParameterList name="ELECTRON_DENSITY">
        <Parameter name="Value" type="string" value="Equilibrium Density"/>
      </ParameterList>
      <ParameterList name="HOLE_DENSITY">
        <Parameter name="Value" type="string" value="Equilibrium Density"/>
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <ParameterList name="Output">
    <!-- <Parameter name="Unscale Variables" type="bool" value="true"/> -->
    <Parameter name="File Name" type="string" value="si-diode.dd.V0.0.exo"/>
    <ParameterList name="Cell Average Quantities">
      <Parameter name="silicon" type="string" value="Doping_Raw,Doping,Donor_Raw Concentration, Acceptor_Raw Concentration,Acceptor Concentration,Donor Concentration,Intrinsic Concentration,Band Gap,Effective Band Gap,Electron Affinity,Effective Electron Affinity,Conduction Band,Valence Band,Intrinsic Fermi Level,Electron Effective DOS,Hole Effective DOS,Reference Energy"/>
    </ParameterList>
  </ParameterList>


    <ParameterList name="Solver Factories">
        <ParameterList name="Rythmos Observers">
            <Parameter name="Output Responses" type="string" value="ON"/>
            <Parameter name="Write Response File" type="string" value="current_time_ddionlatt.csv"/>
        </ParameterList>
    </ParameterList>

    <!-- 
    <ParameterList name="Options">
        <Parameter name="Write Volume Assembly Graphs" type="bool" value="true"/> 
        <Parameter name="Volume Assembly Graph Prefix" type="string" value="charon_"/>
    </ParameterList>
    -->


<ParameterList name="Solution Control">
  <Parameter name="Piro Solver" type="string" value="NOX"/>
  <Parameter name="Compute Sensitivities" type="bool" value="0"/>
  <Parameter name="Jacobian Operator" type="string" value="Have Jacobian"/>
  <ParameterList name="LOCA">
    <ParameterList name="Bifurcation"/>
    <ParameterList name="Constraints"/>
    <ParameterList name="Predictor">
      <Parameter  name="Method" type="string" value="Constant"/>
    </ParameterList>
    <ParameterList name="Stepper">
      <Parameter  name="Continuation Method" type="string" value="Natural"/>
      <Parameter  name="Initial Value" type="double" value="1.0"/>
      <Parameter  name="Continuation Parameter" type="string" value="Parameter 0"/>
      <Parameter  name="Max Steps" type="int" value="6"/>
      <Parameter  name="Max Value" type="double" value="12.25"/>
      <Parameter  name="Min Value" type="double" value="0.5"/>
      <Parameter  name="Compute Eigenvalues" type="bool" value="1"/>
      <ParameterList name="Eigensolver">
        <Parameter name="Method" type="string" value="Anasazi"/>
        <Parameter name="Operator" type="string" value="Shift-Invert"/>
        <Parameter name="Num Blocks" type="int" value="3"/>
        <Parameter name="Num Eigenvalues" type="int" value="1"/>
        <Parameter name="Block Size" type="int" value="1"/>
        <Parameter name="Maximum Restarts" type="int" value="0"/>
        <Parameter name="Shift" type="double" value="1.0"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Step Size">
      <Parameter  name="Initial Step Size" type="double" value="0.5"/>
      <Parameter  name="Aggressiveness" type="double" value="2.0"/>
    </ParameterList>
  </ParameterList>
  <ParameterList name="NOX">
    <ParameterList name="Direction">
      <Parameter name="Method" type="string" value="Newton"/>
      <ParameterList name="Newton">
        <Parameter name="Forcing Term Method" type="string" value="Constant"/>
        <Parameter name="Rescue Bad Newton Solve" type="bool" value="1"/>
        <ParameterList name="Stratimikos Linear Solver">
          <ParameterList name="NOX Stratimikos Options">
          </ParameterList>
          <ParameterList name="Stratimikos">
            <Parameter name="Linear Solver Type" type="string" value="AztecOO"/>
            <ParameterList name="Linear Solver Types">
              <ParameterList name="AztecOO">
                <ParameterList name="Forward Solve"> 
                  <ParameterList name="AztecOO Settings">
                    <Parameter name="Aztec Solver" type="string" value="GMRES"/>
                    <Parameter name="Convergence Test" type="string" value="r0"/>
                    <Parameter name="Size of Krylov Subspace" type="int" value="200"/>
                    <Parameter name="Output Frequency" type="int" value="10"/>
                  </ParameterList>
                  <Parameter name="Max Iterations" type="int" value="500"/>
                  <Parameter name="Tolerance" type="double" value="1e-5"/> 
                </ParameterList>
              </ParameterList>
              <ParameterList name="Belos">
                <Parameter name="Solver Type" type="string" value="Block GMRES"/>
                <ParameterList name="Solver Types">
                  <ParameterList name="Block GMRES">
                    <Parameter name="Convergence Tolerance" type="double" value="1e-5"/>
                    <Parameter name="Output Frequency" type="int" value="1"/>
                    <Parameter name="Output Style" type="int" value="1"/>
                    <Parameter name="Verbosity" type="int" value="31"/>
                    <Parameter name="Maximum Iterations" type="int" value="500"/>
                    <Parameter name="Block Size" type="int" value="1"/>
                    <Parameter name="Num Blocks" type="int" value="20"/>
                    <Parameter name="Flexible Gmres" type="bool" value="1"/>
                  </ParameterList>
                </ParameterList>
              </ParameterList>
            </ParameterList>
            <Parameter name="Preconditioner Type" type="string" value="Ifpack"/>
            <ParameterList name="Preconditioner Types">
              <ParameterList name="Ifpack">
                <Parameter name="Overlap" type="int" value="1"/>
                <Parameter name="Prec Type" type="string" value="ILU"/>
                <ParameterList name="Ifpack Settings">
                  <Parameter name="fact: drop tolerance" type="double" value="0"/>
                  <Parameter name="fact: ilut level-of-fill" type="double" value="1"/>
                  <Parameter name="fact: level-of-fill" type="int" value="1"/>
                </ParameterList>
              </ParameterList>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Line Search">
      <ParameterList name="Full Step">
        <Parameter name="Full Step" type="double" value="1"/>
      </ParameterList>
      <Parameter name="Method" type="string" value="Backtrack"/>
    </ParameterList>
    <Parameter name="Nonlinear Solver" type="string" value="Line Search Based"/>
    <ParameterList name="Printing">
      <Parameter name="Output Precision" type="int" value="3"/>
      <Parameter name="Output Processor" type="int" value="0"/>
      <ParameterList name="Output Information">
        <Parameter name="Error" type="bool" value="1"/>
        <Parameter name="Warning" type="bool" value="1"/>
        <Parameter name="Outer Iteration" type="bool" value="1"/>
        <Parameter name="Parameters" type="bool" value="1"/>
        <Parameter name="Details" type="bool" value="1"/>
        <Parameter name="Linear Solver Details" type="bool" value="1"/>
        <Parameter name="Stepper Iteration" type="bool" value="1"/>
        <Parameter name="Stepper Details" type="bool" value="1"/>
        <Parameter name="Stepper Parameters" type="bool" value="1"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Solver Options">
      <Parameter name="Status Test Check Type" type="string" value="Minimal"/>
    </ParameterList>
    <ParameterList name="Thyra Group Options">
      <Parameter name="Function Scaling" type="string" value="Row Sum"/>
    </ParameterList>
    <ParameterList name="Status Tests">
      <Parameter name="Test Type" type="string" value="Combo"/>
      <Parameter name="Combo Type" type="string" value="OR"/>
      <Parameter name="Number of Tests" type="int" value="2"/>
      <ParameterList name="Test 0">
        <Parameter name="Test Type" type="string" value="NormF"/>
        <Parameter name="Tolerance" type="double" value="1e-10"/>
      </ParameterList>
      <ParameterList name="Test 1">
        <Parameter name="Test Type" type="string" value="MaxIters"/>
        <Parameter name="Maximum Iterations" type="int" value="60"/> 
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <ParameterList name="Rythmos">
    <Parameter name="Nonlinear Solver Type" type="string" value='NOX'/>
    <Parameter name="Final Time" type="double" value="0.1"/>
    <Parameter name="Stepper Type" type="string" value="BDF"/>
    <Parameter name="Step Control Strategy Type" type="string" value="ImplicitBDFRamping"/>
    <Parameter name="Rythmos Integration Control Strategy" type="string" value="Simple"/>

    <ParameterList name="Rythmos Stepper">
      <ParameterList name="VerboseObject">
        <Parameter name="Verbosity Level" type="string" value="medium"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Stratimikos">
    </ParameterList>
    <ParameterList name="Rythmos Integrator">
      <ParameterList name="VerboseObject">
        <Parameter name="Verbosity Level" type="string" value="medium"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Rythmos Integration Control">
      <Parameter name="Take Variable Steps" type="bool" value="true"/>
      <Parameter name="Max dt" type="double" value="0.01" /> 
      <!-- The smaller value of Max dt and Max Step Size is used when both are given !-->
    </ParameterList>

    <ParameterList name="Rythmos Step Control Strategy">
      <Parameter name="Number of Constant First Order Steps" type="int" value="5"/>
      <Parameter name="Initial Step Size" type="double" value="1e-6"/>
      <Parameter name="Min Step Size" type="double" value="1e-14"/>
      <Parameter name="Max Step Size" type="double" value="0.01"/>
      <Parameter name="Step Size Decrease Factor" type="double" value="0.5"/>
      <Parameter name="Step Size Increase Factor" type="double" value="1.5"/>
      <Parameter name="Min Order" type="int" value="1"/>
      <Parameter name="Max Order" type="int" value="1"/>
      <Parameter name="Absolute Error Tolerance" type="double" value="1.0e-6"/>
      <Parameter name="Relative Error Tolerance" type="double" value="1.0e-4"/>
    </ParameterList>
  </ParameterList>  <!--  end "Rythmos" -->
    
</ParameterList>

</ParameterList>

